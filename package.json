{
  "name": "nu-challenge",
  "version": "0.0.1",
  "description": "nu-challenge",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV_DB_BASE=base node index.js",
    "test:windows": "del /f test.json && cross-env NODE_ENV_DB_BASE=test jest --runInBand --coverage",
    "test:unix": "rm -rf ./test.json && cross-env NODE_ENV_DB_BASE=test jest --runInBand --coverage"
  },
  "author": "Rafael Freitas",
  "dependencies": {
    "data-store": "3.1.0",
    "express": "4.17.1"
  },
  "devDependencies": {
    "cross-env": "^5.2.1",
    "jest": "^24.9.0",
    "supertest": "4.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
